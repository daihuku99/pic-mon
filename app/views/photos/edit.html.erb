<div class="photo-edit-wrapprt">
  <div class="photo-edit-form">
    <%= form_with model: @photo, local: :true do |f| %>
      <%= f.label :title %>
      <%= f.text_field :title %>

      <%= f.label :detail %>
      <%= f.text_area :detail %>

      <%= f.file_field :image %>

      <%= f.label :spot %>
      <%= f.text_field :spot %>

      <%= f.label :prefecture %>
      <%= f.collection_select :prefecture_code, JpPrefecture::Prefecture.all, :code, :name %>

      <%= f.label :address_city %>
      <%= f.text_field :address_citye %>

      <%= f.label :address_street %>
      <%= f.text_field :address_street %>

      <%= f.submit 'Update' %>
    <% end %>
  </div>
</div>